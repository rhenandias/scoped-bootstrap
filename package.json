{
  "name": "scoped-bootstrap",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "main": "node src/main.js",
    "css-minify-output": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output output/ --batch --batch-suffix '.min' 'output/bootstrap.css' '!output/bootstrap.min.css'",
    "start": "npm run main && npm run css-minify-output"
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "clean-css-cli": "^5.6.3"
  }
}
